 <%! 
 	// 선언부 : 클래스영역
 	// 클래스변수, 인스턴스 변수, 메서드
	static int num = 5;
 	int num2 = 10;
 	public int multiply(int a, int b){
 		return a * b;
 	}
 %>

num = <%= num %> <br />
num2 = <%= num2 %> <br />
10 * 20 = <%= multiply(10, 20) %> <br />